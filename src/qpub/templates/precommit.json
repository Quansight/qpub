{
    "null": [{
        "repo": "https://github.com/pre-commit/pre-commit-hooks",
        "rev": "v2.3.0",
        "hooks": [{
                "id": "end-of-file-fixer"
            },
            {
                "id": "trailing-whitespace"
            }
        ]
    }],
    ".yml": [{
        "repo": "https://github.com/pre-commit/pre-commit-hooks",
        "hooks": [{
            "id": "check-yaml"
        }]
    }],
    ".py": [{
            "repo": "https://github.com/psf/black",
            "rev": "19.3b0",
            "hooks": [{
                "id": "black"
            }]
        },
        {
            "repo": "https://github.com/life4/flakehell",
            "rev": "v.0.7.0",
            "hooks": [{
                "id": "flakehell"
            }]
        }
    ]
}
